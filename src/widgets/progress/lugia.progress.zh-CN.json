    "type": {
      "desc": "进度条类型，line、circle、dashboard",
      "type": "ProgressType",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": "line"
    },
    "percent": {
      "desc": "百分比",
      "type": "number",
      "designOnly": false,
      "injectProps": false
    },
    "status": {
      "desc": "进度条状态，可设置为success、error或不设",
      "type": "ProgressStatusType",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": "default"
    },
    "active": {
      "desc": "进度条状态是否激活状态，仅生效于线性进度条(type = line)",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "showInfo": {
      "desc": "是否显示进度数值或状态图标",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": true
    },
    "format": {
      "desc": "进度条展示内容模板函数",
      "type": "Function",
      "designOnly": false,
      "injectProps": false
    },
    "showType": {
      "desc": "进度条展示内容展示位置，可设置为 inside 或不设",
      "type": "ProgressShowType",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": "default"
    },
    "iconClass": {
      "desc": "进度条展示的图标,在success或error状态下无配置successIconClass或errorIconClass时生效",
      "type": "icon",
      "designOnly": false,
      "injectProps": false
    },
    "errorIconClass": {
      "desc": "进度条展示的图标,仅在error状态生效",
