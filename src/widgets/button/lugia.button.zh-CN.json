{
  "widgetName": "Button",
  "title": "按钮",
  "aliasName": null,
  "desc": "方便用户点击操作",
  "tag": "按钮 button 按键 主按钮 背景按钮 主要按钮 基本按钮",
  "needExport": true,
  "props": {
    "lugiaHidden": {
      "desc": "是否渲染当前组件",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "disabled": {
      "desc": "按钮是否禁用，true 禁用 false 可用",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "shape": {
      "desc": "设置按钮形状，可以设置为 circle 或者不设",
      "type": "ButtonShape",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": "default"
    },
    "type": {
      "desc": "支持多种不同的按钮风格，可选值为 primary、success、warning、danger 或者不设",
      "type": "ButtonType",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": "primary"
    },
    "plain": {
      "desc": "是否为朴素按钮",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "size": {
      "desc": "设置按钮大小，可选值为 small、large、bigger 或者不设",
      "type": "ButtonSize",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": "default"
    },
    "loading": {
      "desc": "设置按钮加载状态",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "circle": {
      "desc": "设置圆形按钮",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "icon": {
      "desc": "设置按钮前置图标类型",
      "type": "icon",
      "designOnly": false,
      "injectProps": false
    },
    "suffixIcon": {
      "desc": "设置按钮后置图标类型",
      "type": "icon",
      "designOnly": false,
      "injectProps": false
    },
    "text": {
      "desc": "设置按钮的文本内容",
      "type": "string | React.node",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": "Button"
    },
    "block": {
      "desc": "按钮宽度为父元素宽度",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "transition": {
      "desc": "是否开启过渡效果，默认开启true，关闭为false",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": true
    }
  },
  "events": {
    "onClick": {
      "desc": "点击按钮时触发",
      "args": [
        {
          "name": "event",
          "desc": "点击的DOM事件",
          "type": "Object"
        }
      ]
    }
  },
