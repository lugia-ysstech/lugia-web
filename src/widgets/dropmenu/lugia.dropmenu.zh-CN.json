{
  "widgetName": "Dropmenu",
  "title": "下拉菜单",
  "aliasName": null,
  "desc": "选项过多时，弹出下拉菜单给用户选择操作",
  "tag": "下拉菜单 按钮弹出菜单 下拉按钮 Dropmenu 选择按钮 展开菜单 展开按钮菜单",
  "needExport": true,
  "props": {
    "lugiaHidden": {
      "desc": "是否渲染当前组件",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "createPortal": {
      "desc": "是否全局弹出下拉框",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": true
    },
    "popupContainerId": {
      "desc": "配置弹出框的作用容器id",
      "type": "ContainerDomIdType",
      "designOnly": false,
      "injectProps": false
    },
    "size": {
      "desc": "可配置三种尺寸大小的dropmenu",
      "type": "SizeType",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": "default"
    },
    "popupVisible": {
      "desc": "是否展开菜单",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "action": {
      "desc": "弹出项的打开方式",
      "type": "ActionType",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": "click"
    },
    "hideAction": {
      "desc": "弹出项的隐藏方式",
      "type": "ActionType",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": "click"
    },
    "menus": {
      "desc": "弹出项组件",
      "type": "React.Node",
      "designOnly": false,
      "injectProps": false
    },
    "align": {
      "desc": "弹出方向",
      "type": "AlignType",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": "bottom"
    },
    "text": {
      "desc": "下拉菜单按钮的文本",
      "type": "string",
      "designOnly": false,
      "injectProps": false
    },
    "type": {
      "desc": "按钮的风格,默认为customs",
      "type": "StyleType",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": "customs"
    },
    "switchIconClass": {
      "desc": "自定义控制器图标",
      "type": "object",
      "designOnly": false,
      "injectProps": false,
      "meta": [
        {
          "key": "iconClass",
          "title": "图标",
          "type": "icon"
        }
      ]
    },
    "disabled": {
      "desc": "是否禁选",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "divided": {
      "desc": "是否为分割线",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": true
    },
    "showSwitch": {
      "desc": "是否展示switch图标",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": true
    },
    "icons": {
      "desc": "配置前置和后缀图标",
      "type": "object",
      "designOnly": false,
      "injectProps": false,
      "meta": [
        {
          "key": "prefixIconClass",
          "title": "前置图标",
          "type": "icon"
        },
        {
          "key": "suffixIconClass",
          "title": "后缀图标",
          "type": "icon"
        }
      ]
    },
    "data": {
      "desc": "生成选择项的数据",
      "type": "Object[]",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": [
        {
          "value": "选项一",
          "text": "选项一"
        },
        {
          "value": "选项二",
          "text": "选项二"
        },
        {
          "value": "选项三",
          "text": "选项三"
        }
      ],
      "meta": [
        {
          "key": "value",
          "title": "value值",
          "type": "string"
        },
        {
          "key": "text",
          "title": "文本值",
          "type": "string"
        },
        {
          "key": "icons",
          "title": "前/后缀图标",
          "type": "object",
          "children": [
            {
              "key": "prefixIconClass",
              "title": "前缀图标",
              "type": "icon"
            },
            {
              "key": "suffixIconClass",
              "title": "后缀图标",
              "type": "icon"
            },
            {
              "key": "prefixIconSrc",
              "title": "前缀图片",
              "type": "image"
            },
            {
              "key": "suffixIconSrc",
              "title": "后缀图片",
              "type": "image"
            }
          ]
        }
      ]
    }
  },
  "events": {
    "onPopupVisibleChange": {
      "desc": "弹出项展开/隐藏时触发",
      "args": [
        {
          "name": "popupVisible",
          "desc": "展开/隐藏时的popupVisible值",
          "type": "boolean"
        }
      ]
    },
    "onClick": {
      "desc": "点击弹开按钮时触发",
      "args": [
        {
          "name": "event",
          "desc": "点击的DOM事件",
          "type": "Object"
        }
      ]
    },
    "_onClick": {
      "desc": "点击展示框时触发",
      "args": [
        {
          "name": "event",
          "desc": "点击的DOM事件",
          "type": "Object"
        }
      ]
    },
    "onMouseEnter": {
      "desc": "鼠标滑入按钮时触发",
      "args": [
        {
          "name": "event",
          "desc": "点击的DOM事件",
          "type": "Object"
        }
      ]
    },
    "onMouseLeave": {
      "desc": "鼠标离开按钮时触发",
      "args": [
        {
          "name": "event",
          "desc": "点击的DOM事件",
          "type": "Object"
        }
      ]
    },
    "onMenuClick": {
      "desc": "鼠标点击弹开菜单时触发",
      "args": [
        {
          "name": "event",
          "desc": "选中DOM的事件对象",
          "type": "Object"
        },
        {
          "name": "key",
          "desc": "选中项key值",
          "type": "Object"
        },
        {
          "name": "item",
          "desc": "当前选中项的数据",
          "type": "Object"
        }
      ]
    }
  },
