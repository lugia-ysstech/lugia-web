{
  "widgetName": "PageLayout",
  "title": "页面布局",
  "aliasName": null,
  "desc": "快速生成页面布局的组件",
  "tag": "page pagelayout 页面 布局 页面布局",
  "needExport": true,
  "props": {
    "drag": {
      "desc": "页面布局是否支持拖拽切换内容",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "enlarge": {
      "desc": "是否支持指定区域全屏展示",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "fixedHeight": {
      "desc": "区域隐藏后, 组件总高度是否保持不变",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": true
    },
    "fixedContent": {
      "desc": "内容固定,不能通过拖拽分割线改变大小",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "sizeInfo": {
      "desc": "",
      "type": "Object",
      "designOnly": false,
      "injectProps": false,
      "meta": [
        {
          "key": "value",
          "title": "区块对应的宽或高百分比数字",
          "type": "number"
        }
      ]
    },
    "title": {
      "desc": "页面布局组件的title",
      "type": "string",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": "页面布局"
    },
    "hiddenInfo": {
      "desc": "被隐藏区域的信息",
      "type": "Object",
      "designOnly": false,
      "injectProps": false
    },
    "canEdit": {
      "desc": "按F9按钮,是否弹开编辑内容隐藏或显示的编辑",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": true
    }
  },
  "events": {
    "onChange": {
      "desc": "当区域大小发生变化时触发",
      "args": [
        {
          "name": "data",
          "desc": "新的data信息",
          "type": "Object"
        }
      ]
    },
    "onHiddenInfoChange": {
      "desc": "当隐藏对象发生变化时触发",
      "args": [
        {
          "name": "hiddenInfo",
          "desc": "新的hiddenInfo信息",
          "type": "Object"
        }
      ]
    },
    "onContentInfoChange": {
      "desc": "当某区域内容发生改变时触发",
      "args": [
        {
          "name": "contentInfo",
          "desc": "新的contentInfo信息",
          "type": "Object"
        }
      ]
    }
  },
  "designInfo": {},
  "category": [
    "数据录入"
  ],
  "defaultTheme": {
    "Container": {
      "normal": {
        "width": 800
      }
    }
  },
  "theme": {
    "Container": {
      "name": "页面布局",
      "desc": "页面布局组件外盒配置",
      "normal": [
        [
          "width"
        ],
        [
          "height"
        ],
        [
          "background"
        ]
      ],
      "hover": [],
      "clicked": [],
      "disabled": []
    }
  }
}
