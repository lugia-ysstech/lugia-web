{
  "widgetName": "Menu",
  "title": "菜单",
  "aliasName": null,
  "desc": "为用户提供菜单列表",
  "tag": "menu 菜单列表 多列文本",
  "needExport": true,
  "props": {
    "lugiaHidden": {
      "desc": "是否渲染当前组件",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "createPortal": {
      "desc": "是否全局弹出框",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "popupContainerId": {
      "desc": "配置弹出框的作用容器id",
      "type": "containerdomidtype",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "isShowAuxiliaryText": {
      "desc": "是否展示辅助文本",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "auxiliaryTextField": {
      "desc": "辅助文本展示的对应字段名",
      "type": "string",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "valueField": {
      "desc": "data数据的value值的名称",
      "type": "string",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "displayField": {
      "desc": "data数据的displayValue值的名称",
      "type": "string",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "data": {
      "desc": "生成选择项的数据",
      "type": "object[]",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": [
        {
          "value": "选项一",
          "text": "选项一"
        },
        {
          "value": "选项二",
          "text": "选项二"
        },
        {
          "value": "选项三",
          "text": "选项三"
        }
      ],
      "meta": [
        {
          "key": "value",
          "title": "value值",
          "type": "string"
        },
        {
          "key": "text",
          "title": "文本值",
          "type": "string"
        },
        {
          "key": "des",
          "title": "辅助文本",
          "type": "string"
        },
        {
          "key": "icons",
          "title": "前/后缀图标",
          "type": "object",
          "children": [
            {
              "key": "prefixIconClass",
              "title": "前缀图标",
              "type": "icon"
            },
            {
              "key": "suffixIconClass",
              "title": "后缀图标",
              "type": "icon"
            },
            {
              "key": "prefixIconSrc",
              "title": "前缀图片",
              "type": "image"
            },
            {
              "key": "suffixIconSrc",
              "title": "后缀图片",
              "type": "image"
            }
          ]
        }
      ]
    },
    "mutliple": {
      "desc": "是否多选",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "selectedKeys": {
      "desc": "指定当前选中的项",
      "type": "string | string[] | number | number[]",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "defaultSelectedKeys": {
      "desc": "默认指定当前选中的项,仅第一次生效",
      "type": "string | string[] | number | number[]",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "checkedCSS": {
      "desc": "选中项的样式",
      "type": "checkedcsstype",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "limitCount": {
      "desc": "多选时的最大选中数",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "offsety": {
      "desc": "菜单间的间隔",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "start": {
      "desc": "开始展示数据的索引值",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "autoHeight": {
      "desc": "根据data数量，自动计算菜单高度",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "divided": {
      "desc": "项之间是否展示分割线",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "expandedPath": {
      "desc": "层级菜单时展开的数据",
      "type": "string[]",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "separator": {
      "desc": "层级菜单时连接层级数据的分隔符",
      "type": "string",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "offsetX": {
      "desc": "层级菜单时，菜单间的间隔",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "offsetY": {
      "desc": "层级菜单时，子菜单相对父级菜单的top值",
      "type": "string",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "action": {
      "desc": "层级菜单时，展开子菜单的方式",
      "type": "actiontype",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "size": {
      "desc": "可配置三种尺寸大小的menu",
      "type": "sizetype",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "subsize": {
      "desc": "设置子菜单列表项的高度",
      "type": "sizetype",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "align": {
      "desc": "设置菜单弹出方向",
      "type": "string",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": "rightTop"
    }
  },
  "events": {
    "onChange": {
      "desc": "选中项发生变化时触发",
      "args": [
        {
          "name": "keys",
          "desc": "所有的选中值",
          "type": "Object"
        }
      ]
    },
    "onClick": {
      "desc": "点击列表项时触发",
      "args": [
        {
          "name": "event",
          "desc": "选中DOM的事件对象",
          "type": "Object"
        },
        {
          "name": "keys",
          "desc": "所有的选中值",
          "type": "Object"
        },
        {
          "name": "item",
          "desc": "当前选中项的数据",
          "type": "Object"
        }
      ]
    },
    "onMouseEnter": {
      "desc": "鼠标进入列表项时触发",
      "args": [
        {
          "name": "event",
          "desc": "选中DOM的事件对象",
          "type": "Object"
        },
        {
          "name": "item",
          "desc": "当前鼠标进入的列表项数据",
          "type": "Object"
        }
      ]
    },
    "onExpandPathChange": {
      "desc": "层级菜单展开项发生改变时触发",
      "args": [
        {
          "name": "expandedPath",
          "desc": "通过separator连接的各级展开数据",
          "type": "string[]"
        }
      ]
    }
  },
