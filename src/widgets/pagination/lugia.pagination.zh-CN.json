{
  "widgetName": "Pagination",
  "title": "分页",
  "aliasName": null,
  "desc": "分页组件，分页展示数据。",
  "tag": "pagination 分页 分页选择器 页数选择 跳转页面",
  "needExport": true,
  "props": {
    "current": {
      "desc": "当前页数",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "defaultCurrent": {
      "desc": "默认的当前页数",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "total": {
      "desc": "数据总数",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": 1,
      "meta": ""
    },
    "pageSize": {
      "desc": "每页的条数",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "defaultPageSize": {
      "desc": "默认的每页条数",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "pageSizeOptions": {
      "desc": "指定每页可以显示多少条",
      "type": "string[]",
      "designOnly": false,
      "injectProps": false,
      "meta": [
        {
          "key": "pageSizes",
          "title": "每页显示多少条的集合",
          "type": "string"
        }
      ]
    },
    "showQuickJumper": {
      "desc": "是否可以快速跳至某页",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "showTotal": {
      "desc": "用于显示数据总量和当前数据顺序",
      "type": "function(total, range)",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "hideOnSinglePage": {
      "desc": "只有一页时是否隐藏分页器",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "showSizeChanger": {
      "desc": "是否显示可以改变 pageSize",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "showTotalData": {
      "desc": "是否可以显示数据总量",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "simple": {
      "desc": "当添加该属性时，显示为简单分页",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "preIconClass": {
      "desc": "分页上一页图标资源",
      "type": "icon",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "nextIconClass": {
      "desc": "分页下一页图标资源",
      "type": "icon",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "preIconSrc": {
      "desc": "分页上一页图片资源",
      "type": "image",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "nextIconSrc": {
      "desc": "分页下一页图片资源",
      "type": "image",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "blockList": {
      "desc": "分页各部位展示的位置列表",
      "type": "string[]",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": [
        "Page",
        "PageInput",
        "Total",
        "PageSize"
      ],
      "meta": [
        {
          "key": "displayLocation",
          "title": "分页各部位展示的位置",
          "type": "BlockType"
        }
      ]
    },
    "align": {
      "desc": "分页各部位对齐的方式",
      "type": "aligntype",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "size": {
      "desc": "可配置三种尺寸大小的Pagination",
      "type": "sizetype",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "divided": {
      "desc": "是否展示菜单分割线",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "manualQuickJumper": {
      "desc": "是否手动控制分页快速跳转,值为true时,分页跳转由quickJumperValue控制",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "quickJumperValue": {
      "desc": "当manualQuickJumper值为true时,控制分页跳转的值",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "totalText": {
      "desc": "自定义渲染的共计数据文本",
      "type": "string",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "paginationUnitText": {
      "desc": "自定义分页单位文本",
      "type": "string",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "popupContainerId": {
      "desc": "指定Select弹出框的父容器id",
      "type": "string",
      "designOnly": false,
      "injectProps": false
    }
  },
  "events": {
    "onChange": {
      "desc": "页码改变的回调，参数是改变后的页码及每页条数",
      "args": [
        {
          "name": "newValue",
          "desc": "页码改变后的页码",
          "type": "number"
        },
        {
          "name": "oldValue",
          "desc": "页码改变前的页码",
          "type": "number"
        },
        {
          "name": "page",
          "desc": "页码改变后的页码",
          "type": "number"
        },
        {
          "name": "pageSize",
          "desc": "每页条数",
          "type": "number"
        }
      ]
    },
    "onShowSizeChange": {
      "desc": "pageSize 变化的回调",
      "args": [
        {
          "name": "current",
          "desc": "当前页数",
          "type": "number"
        },
        {
          "name": "size",
          "desc": "每页条数",
          "type": "number"
        }
      ]
    },
    "onQuickJumperInputBlur": {
      "desc": "快速跳转输入框失去焦点时触发",
      "args": [
        {
          "name": "current",
          "desc": "当前输入框的值",
          "type": "number"
        },
        {
          "name": "event",
          "desc": "失去焦点的DOM事件",
          "type": "FocusEvent"
        }
      ]
    },
    "onQuickJumperInputEnter": {
      "desc": "键盘按下去并松开后执行",
      "args": [
        {
          "name": "current",
          "desc": "当前输入框的值",
          "type": "number"
        },
        {
          "name": "event",
          "desc": "键盘按下回车键的DOM事件",
          "type": "KeyboardEvent"
        }
      ]
    },
    "onQuickJumperInputChange": {
      "desc": "快速跳转输入框,输入内容改变时触发",
      "args": [
        {
          "name": "event",
          "desc": "改变内容的DOM事件",
          "type": "ChangeType"
        }
      ]
    },
    "onQuickJumperInputKeyUp": {
      "desc": "快速跳转输入框,当键盘按下去并松开后执行",
      "args": [
        {
          "name": "event",
          "desc": "改变键盘输入的内容",
          "type": "KeyboardEvent"
        }
      ]
    },
    "onQuickJumperInputKeyDown": {
      "desc": "快速跳转输入框,当键盘按下任何键时触发",
      "args": [
        {
          "name": "event",
          "desc": "改变键盘输入的内容",
          "type": "KeyboardEvent"
        }
      ]
    },
    "onQuickJumperInputKeyPress": {
      "desc": "快速跳转输入框,当键盘按下任何键时触发",
      "args": [
        {
          "name": "event",
          "desc": "改变键盘输入的内容",
          "type": "KeyboardEvent"
        }
      ]
    },
    "onQuickJumperInputFocus": {
      "desc": "快速跳转输入框,当输入框获得焦点时触发",
      "args": [
        {
          "name": "event",
          "desc": "获取焦点的DOM事件",
          "type": "FocusEvent"
        }
      ]
    }
  },
