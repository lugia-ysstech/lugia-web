        "value": "left",
        "text": "居左"
      },
      {
        "value": "right",
        "text": "居右"
      }
    ],
    "SizeType": [
      { "value": "small", "text": "小" },
      { "value": "default", "text": "正常" },
      { "value": "large", "text": "大" }
    ],
    "ChangeType": {
      "newValue": "number",
      "oldValue": "number",
      "event": "SyntheticEvent"
    }
  },
