{
  "widgetName": "NumberInput",
  "title": "数字输入框",
  "aliasName": null,
  "desc": "常用于数字输入,可以进行快速加减显示",
  "tag": "numberinput 数字输入框 进步器 表单 数值输入框 number输入框 数字调节器",
  "needExport": true,
  "props": {
    "lugiaHidden": {
      "desc": "是否渲染当前组件",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "createPortal": {
      "desc": "是否全局弹出校验框",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "popupContainerId": {
      "desc": "配置弹出校验框的作用容器id",
      "type": "ContainerDomIdType",
      "designOnly": false,
      "injectProps": false
    },
    "disabled": {
      "desc": "禁用状态,是否不可用",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "viewClass": {
      "desc": "用于配置通用主题属性",
      "type": "string",
      "designOnly": false,
      "injectProps": false
    },
    "size": {
      "desc": "可配置三种尺寸大小的input ('大' , '默认' , '小'),高度分别为 40px、32px 和 24px。",
      "type": "InputSize",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": "default"
    },
    "validateStatus": {
      "desc": "校验状态",
      "type": "ValidateStatus",
      "designOnly": false,
      "injectProps": false
    },
    "validateType": {
      "desc": "校验信息显示类型",
      "type": "ValidateType",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": "top"
    },
    "help": {
      "desc": "校验提示信息",
      "type": "string",
      "designOnly": false,
      "injectProps": false
    },
    "placeholder": {
      "desc": "input输入提示信息",
      "type": "string",
      "designOnly": false,
      "injectProps": false
    },
    "defaultValue": {
      "desc": "默认显示内容",
      "type": "string",
      "designOnly": false,
      "injectProps": false
    },
    "value": {
      "desc": "显示内容",
      "type": "string",
      "designOnly": false,
      "injectProps": false
    },
    "max": {
      "desc": "输入最大值限制",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": 999999999999
    },
    "min": {
      "desc": "输入最小值限制",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": -999999999999
    },
    "step": {
      "desc": "每次改变步数，可以为小数",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": 1
    },
    "precision": {
      "desc": "数值精度,默认0",
      "type": "number",
      "designOnly": false,
      "injectProps": false
    },
    "formatter": {
      "desc": "格式化显示内容的匹配规则,需与 parser 属性配套使用",
      "type": "function",
      "designOnly": false,
      "injectProps": false
    },
    "addIcon": {
      "desc": "向上增加的增加图标类型",
      "type": "icon",
      "designOnly": false,
      "injectProps": false
    },
    "subtractIcon": {
      "desc": "向下减少的图标类型",
      "type": "icon",
      "designOnly": false,
      "injectProps": false
    },
    "parser": {
      "desc": "解析格式化显示内容的规则,需与 formatter 属性配套使用",
      "type": "function",
      "designOnly": false,
      "injectProps": false
    },
    "showArrow": {
      "desc": "是否显示步长的箭头图标",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": true
    },
    "suffix": {
      "desc": "后缀图标, showArrow为false时生效",
      "type": "icon",
      "designOnly": false,
      "injectProps": false
    }
  },
  "events": {
    "onClick": {
      "desc": "点击按钮时触发",
      "args": [
        {
          "name": "event",
          "desc": "点击的DOM事件",
          "type": "Object"
        }
      ]
    },
    "onChange": {
      "desc": "内容改变时触发",
      "args": [
        {
          "name": "event",
          "desc": "改变内容的DOM事件",
          "type": "ChangeType"
        }
      ]
    },
    "onKeyUp": {
      "desc": "键盘按下去并松开后执行",
      "args": [
        {
          "name": "event",
          "desc": "改变键盘输入的内容",
          "type": "KeyboardEvent"
        }
      ]
    },
    "onKeyDown": {
      "desc": "键盘按下任何键时触发",
      "args": [
        {
          "name": "event",
          "desc": "改变键盘输入的内容",
          "type": "KeyboardEvent"
        }
      ]
    },
    "onKeyPress": {
      "desc": "键盘按下任何键时触发",
      "args": [
        {
          "name": "event",
          "desc": "改变键盘输入的内容",
          "type": "KeyboardEvent"
        }
      ]
    },
    "onFocus": {
      "desc": "输入框获得焦点时触发",
      "args": [
        {
          "name": "event",
          "desc": "获取焦点的DOM事件",
          "type": "FocusEvent"
        }
      ]
    },
    "onBlur": {
      "desc": "输入框失去焦点时触发",
      "args": [
        {
          "name": "event",
          "desc": "失去焦点的DOM事件",
          "type": "FocusEvent"
        }
      ]
    },
    "onEnter": {
      "desc": "当键入回车时触发事件",
      "args": [
        {
          "name": "event",
          "desc": "当键入回车时触发的事件",
          "type": "KeyboardEvent"
        }
      ]
    },
    "onMouseEnter": {
      "desc": "当鼠标移入输入框内触发",
      "args": [
        {
          "name": "event",
          "desc": "当鼠标移入输入框内触发的事件",
          "type": "MouseEvent"
        }
      ]
    },
    "onMouseLeave": {
      "desc": "当鼠标移出输入框内触发",
      "args": [
        {
          "name": "event",
          "desc": "当鼠标移出输入框外触发的事件",
          "type": "MouseEvent"
        }
      ]
    },
    "getFocus": {
      "desc": "数字输入框主动聚焦事件",
      "args": [
        {
          "name": "focus",
          "desc": "输入框的聚焦方法",
          "type": "Function"
        }
      ]
    }
  },
