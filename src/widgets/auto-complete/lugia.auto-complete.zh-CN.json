      "designOnly": false,
      "injectProps": false
    },
    "size": {
      "desc": "可配置三种尺寸大小的autoComplete",
      "type": "sizeType",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": "default"
    },
    "data": {
      "desc": "生成选择项的数据",
      "type": "string[]",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": [
        "西瓜",
        "桔子",
        "苹果"
      ],
      "meta": [
        {
          "key": "value",
          "title": "value值",
          "type": "string"
        }
      ]
    },
    "createPortal": {
      "desc": "是否全局弹出下拉框",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": true
    },
    "popupContainerId": {
      "desc": "配置弹出框的作用容器id",
      "type": "ContainerDomIdType",
      "designOnly": false,
      "injectProps": false
    },
    "value": {
      "desc": "给定输入框的值",
      "type": "string",
      "designOnly": false,
      "injectProps": false
    },
    "defaultValue": {
      "desc": "默认输入框的值，仅第一次生效",
      "type": "string",
      "designOnly": false,
      "injectProps": false
