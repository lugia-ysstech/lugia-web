      "desc": "是否显示上一次选中的值",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": true
    },
    "placeholder": {
      "desc": "input输入提示信息",
      "type": "string",
      "designOnly": false,
      "injectProps": false
    },
    "validateStatus": {
      "desc": "校验状态",
      "type": "ValidateStatus",
      "designOnly": false,
      "injectProps": false
    },
    "validateType": {
      "desc": "校验信息显示类型",
      "type": "ValidateType",
      "designOnly": false,
      "injectProps": false,
      "propsDefaultValue": "top"
    },
    "prefix": {
      "desc": "输入框前缀",
      "type": "icon",
      "designOnly": false,
      "injectProps": false
    },
    "suffix": {
      "desc": "输入框后缀",
      "type": "icon",
      "designOnly": false,
      "injectProps": false
    },
    "clearIcon": {
      "desc": "清除图标",
      "type": "icon",
      "designOnly": false,
      "injectProps": false
    },
    "oldTimeIcon": {
      "desc": "上次选择值Icon",
      "type": "icon",
      "designOnly": false,
      "injectProps": false
    },
    "help": {
      "desc": "auto-complete校验提示信息",
      "type": "string",
      "designOnly": false,
      "injectProps": false
    }
