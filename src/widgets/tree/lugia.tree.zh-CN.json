                  "normal": [["color"], ["font"]],
                  "hover": [["color"], ["font"]],
                  "disabled": [["color"], ["font"]]
                },
                "CheckboxEdgeChecked": {
                  "name": "Checkbox选中后边框配置",
                  "desc": "Checkbox选中后边框配置样式",
                  "normal": [["background"], ["borderRadius"], ["border"]],
                  "hover": [["background"], ["borderRadius"], ["border"]],
                  "disabled": [["background"], ["borderRadius"], ["border"]]
                },
                "CheckboxEdgeUnChecked": {
                  "name": "Checkbox未选中边框配置",
