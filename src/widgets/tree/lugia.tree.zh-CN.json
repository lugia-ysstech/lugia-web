              "disabled": [["color"], ["font"], ["background"], ["border"], ["borderRadius"]]
            },
            "Switch": {
              "name": "控制器配置",
              "desc": "配置控制树节点展开或隐藏的控制器的样式",
              "normal": [["color"], ["font"]],
              "hover": [["color"], ["font"]],
              "active": [["color"], ["font"]],
              "disabled": [["color"], ["font"]]
            },
            "Checkbox": {
              "name": "多选树的选择框配置",
              "theme": {
                "CheckboxWrap": {
                  "name": "Checkbox整体配置",
