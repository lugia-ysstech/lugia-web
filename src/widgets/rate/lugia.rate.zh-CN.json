{
  "widgetName": "Rate",
  "title": "评分",
  "aliasName": null,
  "desc": "评分组件",
  "tag": "Rate 评分 分数 评价 星级",
  "needExport": true,
  "props": {
    "lugiaHidden": {
      "desc": "是否渲染当前组件",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "count": {
      "desc": "展示的 star 总数",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": 5
    },
    "max": {
      "desc": "最大分值",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": 5
    },
    "disabled": {
      "desc": "禁用状态,不可进行交互",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "allowHalf": {
      "desc": "是否允许半选",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "classify": {
      "desc": "是否区分颜色",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false
    },
    "iconClass": {
      "desc": "自定义图标对象",
      "type": "object",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": {
        "default": "lugia-icon-financial_star",
        "primary": "lugia-icon-financial_star",
        "danger": "lugia-icon-financial_star",
        "amazed": "lugia-icon-financial_star",
        "half": "lugia-icon-financial_half_star"
      },
      "meta": [
        {
          "key": "default",
          "title": "默认图标名称",
          "type": "icon"
        },
        {
          "key": "primary",
          "title": "选中状态图标名称",
          "type": "icon"
        },
        {
          "key": "danger",
          "title": "低分值图标名称",
          "type": "icon"
        },
        {
          "key": "amazed",
          "title": "高分值图标名称",
          "type": "icon"
        },
        {
          "key": "half",
          "title": "半星图标名称",
          "type": "icon"
        }
      ]
    },
    "value": {
      "desc": "当前分值",
      "type": "number",
      "designOnly": false,
      "injectProps": false
    },
    "character": {
      "desc": "自定义展示字符",
      "type": "string | React$Element<any>",
      "designOnly": false,
      "injectProps": false
    }
  },
  "events": {
    "onClick": {
      "desc": "点击时触发",
      "args": [
        {
          "name": "result",
          "desc": "点击后的所选值",
          "type": "Object"
        }
      ]
    },
    "onChange": {
      "desc": "分值改变时触发",
      "args": [
        {
          "name": "result",
          "desc": "鼠标移动时的所选值",
          "type": "Object"
        }
      ]
    }
  },
